# jlet - 20120620: Chequeo de una URL en el protocolo http.
# ARG1: host  
# ARG2: URL
# ARG3: Puerto
define command{
	command_name 	check_http_url
	command_line	/usr/local/nagios/libexec/check_http -H $ARG1$ -u $ARG2$ -p $ARG3$ 
}
# jlet - 20120620: Chequeo de una URL en el protocolo https.
# ARG1: Host 
# ARG2: URL
# ARG3: Puerto
define command{
	command_name 	check_https_url
	command_line	/usr/local/nagios/libexec/check_http -H $ARG1$  -u $ARG2$ -p $ARG3$ --ssl
}

# jlet - 20111110: Monitoreo de la informacion contenida en un archivo generada por
# el comando DF en Linux/Unix
# ARG1: Ruta donde se encuentra el archivo generado por el comando DF
define command{
	command_name    check_filesystem
	command_line    $USER1$/check_filesystem $HOSTADDRESS$ $ARG1$
}		


